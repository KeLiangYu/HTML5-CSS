<!DOCTYPE html>
      <html>
        <head>
          <meta content="text/html; charset=UTF-8" http-equiv="content-type">
          <script type="text/javascript">
      function draw(){
        var c = document.getElementById("myCanvas");
      	var ctx = c.getContext("2d");
        ctx.fillStyle="Red";
      	ctx.beginPath();
      	ctx.arc(60,60,40,0,2*Math.PI);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
        
        ctx.fillStyle="DodgerBlue";
      	ctx.beginPath();
      	ctx.arc(160,60,40,0,2*Math.PI);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
        
        ctx.fillStyle="Yellow";
      	ctx.beginPath();
      	ctx.arc(60,160,40,0,2*Math.PI);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
        
        ctx.fillStyle="Purple";
      	ctx.beginPath();
      	ctx.arc(160,160,40,0,2*Math.PI);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
        
        ctx.fillStyle="Green";
      	ctx.beginPath();
      	ctx.arc(110,110,40,0,2*Math.PI);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
        
        ctx.fillStyle="DodgerBlue";
        ctx.beginPath();
        ctx.moveTo(60,35);
        ctx.lineTo(40,70);
        ctx.lineTo(80,70);
        ctx.fill();
        ctx.closePath();
        
        ctx.fillStyle="Yellow";
        ctx.beginPath();
        ctx.moveTo(160,35);
        ctx.lineTo(140,70);
        ctx.lineTo(180,70);
        ctx.fill();
        ctx.closePath();
        
        ctx.fillStyle="Purple";
        ctx.beginPath();
        ctx.moveTo(60,135);
        ctx.lineTo(40,170);
        ctx.lineTo(80,170);
        ctx.fill();
        ctx.closePath();
        
        ctx.fillStyle="Green";
        ctx.beginPath();
        ctx.moveTo(160,135);
        ctx.lineTo(140,170);
        ctx.lineTo(180,170);
        ctx.fill();
        ctx.closePath();
        
        ctx.fillStyle="Red";
      	ctx.beginPath();
      	ctx.rect(82,98,57,24);
      	ctx.fill();
        ctx.closePath();
      	ctx.stroke();
      }
    </script>
          <title></title>
          <link title="mycss" href="U103B122.css" rel="stylesheet" type="text/css">
        </head>
        <body onload="draw()">
